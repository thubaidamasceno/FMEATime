<?xml version='1.0' encoding='utf-8'?>
<widget id="pro.damasceno.fmeatime" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FMEATime</name>
    <description>
        Aplicativo para axiliar a execução de FMEA
    </description>
    <author email="thubaidamasceno@gmail.com" href="http://fmeatime.damasceno.pro">
        Thubaí Damasceno
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon src="res/android/logoldpi.png" density="ldpi" />
        <icon src="res/android/logomdpi.png" density="mdpi" />
        <icon src="res/android/logohdpi.png" density="hdpi" />
        <icon src="res/android/logoxhdpi.png" density="xhdpi" />
        <icon src="res/android/logoxxhdpi.png" density="xxhdpi" />
        <icon src="res/android/logoxxxhdpi.png" density="xxxhdpi" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

</widget>
